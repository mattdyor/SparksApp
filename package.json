{
  "name": "sparksapp",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:web": "expo export --platform web && node scripts/fix-web-build.js",
    "build:ios": "npx eas-cli build --platform ios --profile production",
    "build:android": "npx eas-cli build --platform android --profile production",
    "build:all": "npx eas-cli build --platform all --profile production",
    "submit:ios": "npx eas-cli submit --platform ios",
    "submit:android": "npx eas-cli submit --platform android",
    "deploy:web": "expo export --platform web"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.8",
    "@types/styled-components": "^5.1.34",
    "expo": "^54.0.9",
    "expo-audio": "~1.0.13",
    "expo-av": "^16.0.7",
    "expo-document-picker": "~14.0.7",
    "expo-file-system": "~19.0.15",
    "expo-haptics": "~15.0.7",
    "expo-image-picker": "~17.0.8",
    "expo-media-library": "~18.2.0",
    "expo-modules-core": "^3.0.17",
    "expo-notifications": "~0.32.11",
    "expo-sharing": "~14.0.7",
    "expo-speech": "~14.0.7",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-circular-progress-indicator": "^4.4.2",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "styled-components": "^6.1.19",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.1.10",
    "typescript": "~5.9.2"
  },
  "private": true
}
